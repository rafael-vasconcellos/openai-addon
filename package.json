    "jsonrepair": "^3.10.0",
    "openai": "^4.77.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.11",
    "axios": "^1.7.9",
    "esbuild": "^0.25.1",
    "tsc-alias": "^1.8.11",
    "typescript": "^5.7.0",
    "unzipper": "^0.12.3",
    "vitest": "^3.0.9"
  }
}
