{
  "name": "openai-addon",
  "title": "OpenAI",
  "description": "OpenAI/g4f support for Translator++.",
  "icon": "icon.png",
  "author": {
    "name": "github.com/rafael-vasconcellos"
  },
  "version": "1.0.0",
  "main": "main.js",
  "autoload": false,
  "load": [
    "main.js"
  ],
  "scripts": {
    "build": "tsc && node esbuild.config.mjs && tsc-alias",
